{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "bin",
      "program": "${workspaceRoot}/bin/minified-size",
      "args": [
        "test/*"
      ],
      "skipFiles": [
        "<node_internals>/**/*.js",
        "node_modules/async-hook-domain/index.js",
        "node_modules/@nodelib/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "test",
      "program": "${workspaceRoot}/test/test",
      "skipFiles": [
        "<node_internals>/**/*.js",
        "node_modules/async-hook-domain/index.js",
        "node_modules/@nodelib/**/*.js"
      ]
    }
  ]
}
