{
  "name": "minified-size",
  "version": "3.0.0",
  "description": "Estimates the size of minified and gzipped/brotlied JavaScript, CSS and HTML files.",
  "author": {
    "name": "Ferdinand Prantl",
    "email": "prantlf@gmail.com",
    "url": "http://prantl.tk"
  },
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/prantlf/minified-size/blob/master/LICENSE"
    }
  ],
  "homepage": "https://github.com/prantlf/minified-size#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/prantlf/minified-size.git"
  },
  "bugs": {
    "url": "https://github.com/prantlf/minified-size/issues"
  },
  "engines": {
    "node": ">=10"
  },
  "main": "lib/index.js",
  "bin": {
    "minified-size": "./bin/minified-size.js"
  },
  "files": [
    "bin",
    "lib"
  ],
  "scripts": {
    "lint": "denolint",
    "check": "tap --100",
    "test": "denolint && tap --100"
  },
  "tap": {
    "browser": false,
    "coverage-report": [
      "text-summary",
      "lcov"
    ]
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/github",
        {
          "failComment": false
        }
      ],
      "@semantic-release/git"
    ]
  },
  "dependencies": {
    "@prantlf/crass": "0.12.6",
    "@swc/core": "^1.2.220",
    "babel-minify": "0.5.2",
    "colorette": "^2.0.19",
    "commander": "9.4.0",
    "esbuild": "0.14.51",
    "fast-glob": "3.2.11",
    "html-minifier": "4.0.0",
    "pretty-bytes": "5.6.0",
    "terser": "5.14.2"
  },
  "devDependencies": {
    "@node-rs/deno-lint": "^1.14.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "tap": "16.3.0"
  },
  "keywords": [
    "size-calculation",
    "gziped-files",
    "brotlied-files",
    "estimate",
    "minify",
    "gzip",
    "brotli",
    "script",
    "javascript",
    "js",
    "stylesheet",
    "css",
    "html",
    "files",
    "size"
  ]
}
