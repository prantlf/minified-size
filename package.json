{
  "name": "minified-size",
  "version": "2.2.1",
  "description": "Estimates the size of minified and gzipped JavaScript, CSS and HTML files.",
  "author": {
    "name": "Ferdinand Prantl",
    "email": "prantlf@gmail.com",
    "url": "http://prantl.tk"
  },
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/prantlf/minified-size/blob/master/LICENSE"
    }
  ],
  "homepage": "https://github.com/prantlf/minified-size#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/prantlf/minified-size.git"
  },
  "bugs": {
    "url": "https://github.com/prantlf/minified-size/issues"
  },
  "engines": {
    "node": ">=10"
  },
  "main": "lib/index.js",
  "bin": {
    "minified-size": "./bin/minified-size"
  },
  "files": [
    "bin",
    "lib"
  ],
  "scripts": {
    "lint": "standard --verbose bin/minified-size lib/*.js test/*.js",
    "lint:fix": "standard --verbose --fix bin/minified-size lib/*.js test/*.js",
    "check": "tap --100",
    "test": "npm run lint && npm run check"
  },
  "tap": {
    "browser": false,
    "coverage-report": [
      "text-summary",
      "lcov"
    ]
  },
  "dependencies": {
    "@prantlf/crass": "0.12.6",
    "babel-minify": "0.5.1",
    "chalk": "4.1.1",
    "commander": "7.2.0",
    "esbuild": "0.12.6",
    "fast-glob": "3.2.5",
    "html-minifier": "4.0.0",
    "pretty-bytes": "5.6.0",
    "terser": "5.7.0"
  },
  "devDependencies": {
    "eslint": "7.28.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-standard": "4.1.0",
    "standard": "16.0.3",
    "tap": "15.0.9"
  },
  "keywords": [
    "size-calculation",
    "gziped-files",
    "estimate",
    "minify",
    "gzip",
    "script",
    "javascript",
    "js",
    "stylesheet",
    "css",
    "html",
    "files",
    "size"
  ]
}
